//Write a Program to input a number and print whether it is a even or odd.

using System;

class Program {
  static void Main() {
      Console.Write("Enter a number: ");
      int num = Convert.ToInt32(Console.ReadLine());
      if (num % 2 == 0) {
           Console.WriteLine("Even");
        } else {
          Console.WriteLine("Odd");
    }
  }
}
